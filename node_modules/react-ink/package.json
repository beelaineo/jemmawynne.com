    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "eslint": "^4.19.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-react": "^7.8.2",
    "prettier": "^1.13.2",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "webpack": "^4.10.0",

{
	"name": "root",
	"private": true,
	"scripts": {
		"setup": "yarn install",
		"test": "yarn workspace jemmawynne-app test",
		"build": "yarn workspace jemmawynne-app build && yarn workspace jemmawynne-proxy build",
		"start:app": "yarn workspace jemmawynne-app start",
		"start:proxy": "yarn workspace jemmawynne-proxy start"
	},
	"devDependencies": {
		"@commitlint/cli": "^8.2.0",
		"@commitlint/config-conventional": "^8.2.0",
		"@typescript-eslint/eslint-plugin": "^2.6.1",
		"@typescript-eslint/parser": "^2.6.1",
		"dotenv": "^8.2.0",
		"eslint": "^6.6.0",
		"eslint-config-prettier": "^6.5.0",
		"eslint-plugin-prettier": "^3.1.1",
		"eslint-plugin-react": "^7.16.0",
		"husky": "^3.0.9",
		"lerna": "^3.18.4",
		"netlify-cli": "^2.20.2",
		"prettier": "^1.19.1",
		"pretty-quick": "^2.0.1",
		"typescript": "^3.7.2",
		"yarnhook": "^0.4.3"
	},
	"workspaces": [
		"app",
		"sanity",
		"proxy"
	],
	"husky": {
		"hooks": {
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
			"pre-commit": "pretty-quick --staged",
			"post-checkout": "yarnhook",
			"post-merge": "yarnhook",
			"post-rewrite": "yarnhook"
		}
	},
	"dependencies": {
		"react-icons": "^3.8.0"
	},
	"resolutions": {
		"graphql": "14.5.8"
	}
}
